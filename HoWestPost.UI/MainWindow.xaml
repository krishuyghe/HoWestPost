<Window x:Class="HoWestPost.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HoWestPost.UI"
        mc:Ignorable="d"
        Title="HoWest Post" Height="450" Width="800" Loaded="MainWindow_Loaded">
    <Grid x:Name="lblTimeLeft">
        <CheckBox x:Name="CheckBoxPrior" Content="Prior?" HorizontalAlignment="Left" Margin="69,90,0,0" VerticalAlignment="Top" Checked="CheckBoxPrior_Checked" />
        <Button x:Name="ButtonMini" Content="mini" HorizontalAlignment="Left" Margin="10,136,0,0" VerticalAlignment="Top" Width="57" Click="ButtonMini_Click"/>
        <ListBox x:Name="ListBoxWaiting" HorizontalAlignment="Left" Height="164" Margin="259,78,0,0" VerticalAlignment="Top" Width="237" AutomationProperties.Name="listBussy"/>
        <Button x:Name="ButtonStandaard" Content="standaard" HorizontalAlignment="Left" Margin="72,136,0,0" VerticalAlignment="Top" Width="75" Click="ButtonStandaard_Click"/>
        <Button x:Name="buttonMaxi" Content="maxi" HorizontalAlignment="Left" Margin="160,136,0,0" VerticalAlignment="Top" Width="75" Click="buttonMaxi_Click"/>
        <Label Content="Reistijd" HorizontalAlignment="Left" Margin="18,49,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblTravelTime"/>
        <Label Content="Packet Versturen" HorizontalAlignment="Left" Margin="62,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <Label Content="In Behandeling" HorizontalAlignment="Left" Margin="284,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <Label Content="Onderweg" HorizontalAlignment="Left" Margin="570,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <Image HorizontalAlignment="Left" Height="82" Margin="587,53,0,0" VerticalAlignment="Top" Width="115"/>
        <Image Margin="570,52,114,284" Source="Image1.png" Stretch="Fill" RenderTransformOrigin="1.769,1.202"/>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Left" Height="10" Margin="570,160,0,0" VerticalAlignment="Top" Width="166" AutomationProperties.Name="progresTime"/>
        <Label Content="Totale reistijd" HorizontalAlignment="Left" Margin="570,191,0,0" VerticalAlignment="Top" Width="99"/>
        <Label Content="Post type" HorizontalAlignment="Left" Margin="570,228,0,0" VerticalAlignment="Top" Width="78"/>
        <Label Content="Pior" HorizontalAlignment="Left" Margin="570,267,0,0" VerticalAlignment="Top"/>
        <Label Content="Resterent" HorizontalAlignment="Left" Margin="570,307,0,0" VerticalAlignment="Top"/>
        <Label HorizontalAlignment="Left" Margin="664,191,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblTotalTravelTime" FontWeight="Bold"/>
        <Label x:Name="lblType" Content="lblPostType" HorizontalAlignment="Left" Margin="664,228,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblPostType" FontWeight="Bold"/>
        <Label x:Name="lblPrior" Content="lblPrior" HorizontalAlignment="Left" Margin="664,267,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblPrior" FontWeight="Bold"/>
        <Label x:Name="lblTimeLeft1" Content="lblTimeleft" HorizontalAlignment="Left" Margin="664,307,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblResterent" FontWeight="Bold"/>
        <Label x:Name="lblTotalTravelTime" Content="lblTotalTravelTime" HorizontalAlignment="Left" Margin="664,191,0,0" VerticalAlignment="Top" AutomationProperties.Name="lblTotalTravelTime" FontWeight="Bold"/>
        <ComboBox IsSelected="True" x:Name="ComboxDeliveryTime" HorizontalAlignment="Left" Margin="84,52,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboxDeliveryTime_SelectionChanged"/>
        <Label x:Name="lblTime" Content="Label" HorizontalAlignment="Left" Margin="50,208,0,0" VerticalAlignment="Top" Width="97"/>

    </Grid>
</Window>
